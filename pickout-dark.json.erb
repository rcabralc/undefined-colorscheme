{
    "theme": {
      "font-family": "<%= fontname %>",
      "font-size": "14px",
      "prompt-font-size": "20px",
      "entries-preformatted-font-family": "<%= fontname %>",
      "background-color": "<%= Undefined.dark.bg %>",
      "prompt-background-color": "<%= Undefined.dark.bg %>",
      "prompt-color": "<%= Undefined.dark.cyan0 %>",
      "prompt-over-limit-color": "<%= Undefined.dark.red0 %>",
      "input-color": "<%= Undefined.dark.fg %>",
      "input-not-found-color": "<%= Undefined.dark.red0 %>",
      "input-history-color": "<%= Undefined.dark.orange0 %>",
      "counters-color": "<%= Undefined.dark.lime0 %>",
      "input-background-color": "<%= Undefined.dark.bg %>",
      "entries-background-color-gradient-start": "<%= format("rgba(%d,%d,%d,0)", *Undefined.dark.bg) %>",
      "entries-background-color-gradient-end": "<%= format("rgba(%d,%d,%d,1)", *Undefined.dark.bg) %>",
      "entries-color": "<%= Undefined.dark.gray4 %>",
      "entries-alternate-color": "<%= Undefined.dark.gray4 %>",
      "entries-hl-color": "<%= Undefined.dark.red0 %>",
      "entries-alternate-hl-color": "<%= Undefined.dark.red0 %>",
      "entries-background-color": "<%= Undefined.dark.altbg %>",
      "entries-hl-background-color": "<%= Undefined.dark.altbg %>",
      "entries-alternate-background-color": "<%= Undefined.dark.bg %>",
      "entries-alternate-hl-background-color": "<%= Undefined.dark.bg %>",
      "entries-text-shadow": "<%= defined?(textshadow) ? textshadow : '0 2px 6px rgba(0, 0, 0, 0.63)' %>",
      "selected-entry-color": "<%= Undefined.dark.fg %>",
      "selected-entry-hl-color": "<%= Undefined.dark.red0 %>",
      "selected-entry-background-color": "<%= Undefined.dark.gray1 %>",
      "selected-entry-hl-background-color": "<%= Undefined.dark.gray1 %>"
    }
}
