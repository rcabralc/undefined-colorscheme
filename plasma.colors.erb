<%
    require_relative "./#{name.downcase}"
    scheme = Undefined.const_get(name)
    palette = scheme.public_send(variant)
    description = variant == 'dark' ? "#{name}Dark" : "#{name}Light"
    compl_variant = variant == 'dark' ? :light : :dark
    compl_palette = scheme.public_send(compl_variant)
%>[ColorEffects:Disabled]
